alien_color = 'green'
if alien_color == 'green':
    print("You have just earned 5 points!")
alien_color = 'red'
if alien_color == 'green':
    print("You have just earned 5 points!")

alien_color = 'green'
if alien_color == 'green':
    print("You have just earned 5 points!")
else:
    print("You have just earned 10 points!")
alien_color = 'red'
if alien_color == 'green':
    print("You have just earned 5 points!")
else:
    print("You have just earned 10 points!")

alien_color = 'green'
if alien_color == 'green':
    print("You have just earned 5 points!")
elif alien_color == 'yellow':
    print("You have just earned 10 points!")
else:
    print("You have just earned 15 points!")
alien_color = 'yellow'
if alien_color == 'green':
    print("You have just earned 5 points!")
elif alien_color == 'yellow':
    print("You have just earned 10 points!")
else:
    print("You have just earned 15 points!")
alien_color = 'red'
if alien_color == 'green':
    print("You have just earned 5 points!")
elif alien_color == 'yellow':
    print("You have just earned 10 points!")
else:
    print("You have just earned 15 points!")

age = 22
if age < 2:
    print("You are a baby.")
elif age < 4:
    print("You are a toddler.")
elif age < 13:
    print("You are a kid.")
elif age < 20:
    print("You are a teenager.")
elif age < 65:
    print("You are an adult.")
else:
    print("You are an elder.")

favorite_fruits = ['apple', 'strawberry', 'grape']
if 'apple' in favorite_fruits:
    print("I love eating apple!")
if 'strawberry' in favorite_fruits:
    print("I love eating strawberry!")
if 'banana' in favorite_fruits:
    print("I love eating banana!")
if 'grape' in favorite_fruits:
    print("I love eating grape!")
if 'blueberry' in favorite_fruits:
    print("I love eating blueberry!")

usernames = ['admin', 'david', 'root', 'user', 'williams']
for usernames in usernames:
    if usernames == 'admin':
        print(f"Hello {usernames}, would you like to see a status report?")
    else:
        print(f"Hello {usernames}, thank you for logging in again.")

usernames = []
if usernames:
    for usernames in usernames:
        print('Hello {usernames}')
else:
    print("We need to find some users!")

current_users = ['jack', 'jim', 'bob', 'james', 'tom']
new_users = ['charlie', 'jack', 'rylie', 'james', 'kate']
for new_users in new_users:
    if new_users in current_users:
        print("Username already taken!")
    else:
        print("Username is available!")

ordinal_numbers = list(range(1, 10))
for ordinal_numbers in ordinal_numbers:
    if ordinal_numbers == 1:
        print(f"{ordinal_numbers}st")
    elif ordinal_numbers == 2:
        print(f"{ordinal_numbers}nd")
    elif ordinal_numbers == 3:
        print(f"{ordinal_numbers}rd")
    else:
        print(f"{ordinal_numbers}th")
